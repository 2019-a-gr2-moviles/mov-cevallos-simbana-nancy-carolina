<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MenuActivity" android:background="@mipmap/sakura">

    <Button
            android:text="Gestionar Papas"
            android:layout_width="111dp"
            android:layout_height="67dp"
            android:id="@+id/btn_gestionPapas" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginTop="104dp"
            android:background="#3F51B5"/>
    <Button
            android:text="Crear Papa"
            android:layout_width="100dp"
            android:layout_height="76dp"
            android:id="@+id/btn_crearPapa"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/btn_gestionPapas"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/btn_gestionPapas"
            app:layout_constraintVertical_bias="0.337" android:layout_marginBottom="8dp" android:background="#3F51B5"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/btn_gestionPapas"
            android:layout_marginRight="8dp"
            app:layout_constraintHorizontal_bias="1.0"/>
</android.support.constraint.ConstraintLayout>